<%= form_with model: @application, action: "/applications/", local: true, builder: LabellingFormBuilder do |form| %>
  <%= form.text_field :name %><br>
  <%= form.text_field :street %><br>
  <%= form.text_field :city %><br>
  <%= form.text_field :state %><br>
  <%= form.text_field :zip_code %><br>
  <%= form.text_field :description %><br>
  <%= form.submit "Submit" %>
<% end %>
