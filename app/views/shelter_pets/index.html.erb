<h1><%= link_to "#{@shelter.name}", show_shelter_path(@shelter.id) %>'s Pets</h1>
<%= link_to "Add Pet", new_shelter_pets_path(@shelter.id) %><br>
<% @shelter.pets.each do |pet| %>
<h2>Name:<%= link_to "#{pet.name}", "pets/#{pet.id}" %></h2>
  <%= image_tag "#{pet.image}" %><br>
  Approx Age:<%= pet.approximate_age %><br>
  Sex:<%= pet.sex %><br>
<% end %>
